<?xml version="1.0" encoding="utf-8"?>

<robot name="hoverboard" >

  <material name="rr_m_green">
    <color rgba="0 1 0 1"/>
  </material>
  <material name="rr_m_green_tp">
    <color rgba="0 1 0 .5"/>
  </material>
  <material name="rr_m_black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="rr_m_gray_tp">
    <color rgba="0.5 0.5 0.5 .9"/>
  </material>
  <material name="rr_m_gray">
    <color rgba="0.7 0.7 0.7 1"/>
  </material>

  <material name="darker_blue">
    <color rgba="0.5 0.5 1 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="blue">
    <color rgba="0.2 0.2 1 1"/>
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.092 0.092 0.042"/>
      </geometry>
      <origin xyz="0 0 0.015"/>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.092 0.092 0.042"/>
      </geometry>
      <origin xyz="0 0 0.015"/>
    </collision>
    <inertial>
      <!-- place the centre of mass in the actual centre of the link (not its origin) -->
      <origin xyz="0 0 0.015"/>
      <mass value="0.3"/>
      <inertia ixx="0.0002557" ixy="0.0" ixz="0.0" iyy="0.0002557" iyz="0.0" izz="0.0004232"/>
    </inertial>
  </link>
  <gazebo reference="base_link">
    <material>Gazebo/Blue</material>
    <selfCollide>false</selfCollide>
  </gazebo>
  
  <!-- ################# LEFT WHEEL ############################ -->
  <joint name="left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel"/>
    <origin rpy="0 1.57079632679 1.57079632679" xyz="0 0.0365 0"/>
    <axis rpy="0 0 0" xyz="0 0 1"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>

  <link name="left_wheel">
    <visual>
      <geometry>
        <cylinder length="0.021" radius="0.015"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.021" radius="0.015"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <inertia ixx="7.51875e-06" ixy="0.0" ixz="0.0" iyy="7.51875e-06" iyz="0.0" izz="2.25e-05"/>
    </inertial>
  </link>
  <gazebo reference="left_wheel">
    <material>Gazebo/CoM</material>
    <mu1 value="1.0e+6"/>
    <mu2 value="1.0e+6"/>
    <kp value="1.0e+9"/>
    <kd value="5.0e+6"/>
    <!--
    <mu1 value="200"/>
    <mu2 value="100"/>
    <kp value="10000000.0"/>
    <kd value="1.0"/>
    -->
    <selfCollide>false</selfCollide>
  </gazebo>

  <!-- ################# RIGHT WHEEL ############################ -->

  <joint name="right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="right_wheel"/>
    <origin rpy="0 1.57079632679 1.57079632679" xyz="0 -0.0365 0"/>
    <axis rpy="0 0 0" xyz="0 0 1"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>
  <link name="right_wheel">
    <visual>
      <geometry>
        <cylinder length="0.021" radius="0.015"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.021" radius="0.015"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <inertia ixx="7.51875e-06" ixy="0.0" ixz="0.0" iyy="7.51875e-06" iyz="0.0" izz="2.25e-05"/>
    </inertial>
  </link>
  <gazebo reference="right_wheel">
    <material>Gazebo/CoM</material>
    <mu1 value="1.0e+6"/>
    <mu2 value="1.0e+6"/>
    <kp value="1.0e+9"/>
    <kd value="5.0e+6"/>
    <!--
    <mu1 value="200"/>
    <mu2 value="100"/>
    <kp value="10000000.0"/>
    <kd value="1.0"/>
    -->
    <selfCollide>false</selfCollide>
  </gazebo>

  <!-- ################# FRONTAL CASTER WHEEL ############################ -->
  <joint name="base_to_castor_frontal_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="castor_frontal_wheel"/>
    <origin xyz="0.037 0 -0.006"/>
  </joint>

  <link name="castor_frontal_wheel">
    <visual>
      <geometry>
        <sphere radius="0.009"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.009"/>
      </geometry>
      <surface>
        <friction>
        <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
        </ode>
        </friction>
    </surface>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <inertia ixx="6.48e-06" ixy="0.0" ixz="0.0" iyy="6.48e-06" iyz="0.0" izz="6.48e-06"/>
    </inertial>
  </link>
  <gazebo reference="castor_frontal_wheel">
    <mu1 value="0.0"/>
    <mu2 value="0.0"/>
    <kp value="1.0e+9"/>
    <kd value="5.0e+6"/>
    <material>Gazebo/Black</material>
  </gazebo>

  <!-- ################# REAR CASTER WHEEL ############################ -->
  <joint name="base_to_castor_rear_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="castor_rear_wheel"/>
    <origin xyz="-0.037 0 -0.006"/>
  </joint>

  <link name="castor_rear_wheel">
    <visual>
      <geometry>
        <sphere radius="0.009"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.009"/>
      </geometry>
      <surface>
        <friction>
        <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
        </ode>
        </friction>
    </surface>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <inertia ixx="6.48e-06" ixy="0.0" ixz="0.0" iyy="6.48e-06" iyz="0.0" izz="6.48e-06"/>
    </inertial>
  </link>
  <gazebo reference="castor_rear_wheel">
    <mu1 value="0.0"/>
    <mu2 value="0.0"/>
    <kp value="1.0e+9"/>
    <kd value="5.0e+6"/>
    <material>Gazebo/Black</material>
  </gazebo>
 

  <!-- ################# DEPTH CAMERA ############################ -->

  <joint name="camera_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz="0.05 0 0.04" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="camera_link"/>
  </joint>

  <link name="camera_link">
      <collision>
         <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.03 0.01 0.01"/>
        </geometry>
      </collision>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.03 0.03 0.01"/>
        </geometry>
      </visual>
      <inertial>
        <mass value="1e-5" />
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
      </inertial>
  </link>


  <!-- hokuyo gazebo references -->
  <gazebo reference="camera_link">
    <sensor type="depth" name="camera1">
      <update_rate>60</update_rate>
      <visualize>false</visualize>
      <camera>
        <horizontal_fov>1.92</horizontal_fov>
        <image>
          <width>640</width>
          <height>480</height>
          <format>B8G8R8</format>
        </image>
        <depth_camera>
        </depth_camera>
        <clip>
          <near>0.02</near>
          <far>300</far>
        </clip>
        <noise>
          <type>gaussian</type>
          <!-- Noise is sampled independently per pixel on each frame.
                That pixel's noise value is added to each of its color
                channels, which at that point lie in the range [0,1]. -->
          <mean>0.0</mean>
          <stddev>0.007</stddev>
        </noise>
      </camera>
      <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
        <ros>
          <namespace>sim_cam</namespace>

          <!-- topics need to be prefixed with camera_name for remapping -->
          <!-- <remapping>mycamera/image_raw:=mycamera/image_demo</remapping>
          <remapping>mycamera/depth/image_raw:=mycamera/depth_demo</remapping>
          <remapping>mycamera/camera_info:=mycamera/raw_cam_info_demo</remapping>
          <remapping>mycamera/depth/camera_info:=mycamera/depth_cam_info_demo</remapping>
          <remapping>mycamera/points:=mycamera/points_demo</remapping> -->
        </ros>

        <!-- omit camera_name to default to sensor name -->
        <camera_name>mycamera</camera_name>

        <!-- frame_name>omit so it defaults to link name</frame_name-->

        <min_depth>0.1</min_depth>
        <max_depth>500</max_depth>
      </plugin>
    </sensor>
  </gazebo>

  <gazebo>
    <plugin name="joint_states" filename="libgazebo_ros_joint_state_publisher.so">
      <ros>
        <!-- <namespace>/</namespace> -->
          <argument>--ros-args</argument>
          <argument>-r </argument>
          <argument>~/out:=joint_states</argument>
      </ros>
      <update_rate>50</update_rate>
      <joint_name>right_wheel_joint</joint_name>
      <joint_name>left_wheel_joint</joint_name>
    </plugin>

    <plugin name="hoverboard_diff_drive" filename="libgazebo_ros_diff_drive.so">

        <ros>
          <!-- <namespace>/sim</namespace> -->
        </ros>

      <update_rate>50</update_rate>
      <left_joint>left_wheel_joint</left_joint>
      <right_joint>right_wheel_joint</right_joint>
      <wheel_separation>0.073</wheel_separation>
      <wheel_diameter>0.03</wheel_diameter>
      <command_topic>cmd_vel</command_topic>

      <!-- output -->
      <publish_odom>true</publish_odom>
      <publish_odom_tf>true</publish_odom_tf>
      <publish_wheel_tf>true</publish_wheel_tf>

      <odometry_topic>odom</odometry_topic>
      <odometry_frame>odom</odometry_frame>
      <robot_base_frame>base_link</robot_base_frame>

      <max_wheel_torque>30</max_wheel_torque>
      <max_wheel_acceleration>1.0</max_wheel_acceleration>
    </plugin>
  </gazebo>
</robot>
